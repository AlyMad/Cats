def main():

  favorite_cats = []

  print("Enter your favorite color of cat(seperated by spaces, capitalized):")
  cats_input = input().split()

  favorite_cats.extend(cats_input)

  if 'Tabby' in favorite_cats:
    print("Great choice I have a tabby cat!")

    print("Your favorites:")
    for cat in favorite_cats:
      if cat == 'Tabby':
        print(f"*{cat}")
      else:
        print(f" {cat}")

    print("One of my favorites too.")
  else:
    print("Your favorites:")
    for cat in favorite_cats:
      print(f" {cat}")

if __name__ == "__main__":
  main()
